{
  "name": "Lupusec2Mqtt",
  "version": "1.0.3",
  "slug": "Lupusec2Mqtt",
  "description": "Connect your Lupusec Alarm system to HomeAssistant by an MQTT integration",
  "startup": "application",
  "init": false,
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "boot": "auto",
  "options": {
    "Mqtt:Server": "192.168.1.110",
    "Mqtt:Login": "",
    "Mqtt:Password": "",
    "Lupusec:Url": "http://192.168.1.111",
    "Lupusec:Login": "admin",
    "Lupusec:Password": "secret"
  },
  "schema": {
    "Mqtt:Server": "str",
    "Mqtt:Login": "str",
    "Mqtt:Password": "str",
    "Lupusec:Url": "str",
    "Lupusec:Login": "str",
    "Lupusec:Password": "str"
  },
  "image": "cyberdns/lupusec2mqtt_hassaddon_{arch}"
}
